/**app.wxss**/
view,text,navigator{
  margin:0;
  padding:0;
  color:#333;
  letter-spacing: 2rpx;
  line-height: 1;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.text-right{
  text-align: right;
}
.float-left{
  float:left;
}
.float-right{
  float:right;
}
navigator{
  color:#bf9a5c;
}
.orange{
  color:#ff9933;
}
.page-cover{
  display: none;
  position: fixed;
  left:0;
  top:0;
  width: 100%;
  height: 100%;
  background:rgba(0,0,0,0.51);
  z-index: 10;
}
.page-cover.show{
  display: block;
}
/**foot.template**/
.page-foot{
  display: flex;
  justify-content: center;
  margin: 100rpx 0 20rpx 0;
  text-align: center;
  font-size: 9pt;
}
.foot-text{
  margin:0 30rpx;
  color:#333;
}
.copyright{
  margin: 20rpx 0;
  text-align: center;
  color: #999;
  font-size: 9pt;
}

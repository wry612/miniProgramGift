<!--pages/activityList/activityList.wxml-->
<navigator url='../activity/activity?convertObjId={{item.convertObjId}}' class='activity-item' wx:for="{{activityList}}">
  <view>
    <image src='https://gifttest.jsdttec.com/main/static/images/view/{{item.icon}}' style='width:100%;height:386rpx;'></image>
  </view>
  <view class='desc'>
    <view>活动时间：{{item.startTime}}—{{item.endTime}}</view>
    <view><text>查看详情</text>  <image src='http://192.168.7.6:8080/static/xccImage/small-arrow.png' style='width:12rpx;height:20rpx;margin-left:10rpx;'></image></view>
  </view>
</navigator>